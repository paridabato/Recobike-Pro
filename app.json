{
  "expo": {
    "name": "Recobike Pro",
    "slug": "recobike-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon-recobike-2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recobike.pro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon-recobike-2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.recobike.pro"
    },
    "web": {
      "favicon": "./assets/favicon-recobike-2.png"
    },
    "extra": {
      "eas": {
        "projectId": "29fa82ef-9f79-4c88-8398-97f5cbbe6bf9"
      }
    },
    "owner": "parida01",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ]
  }
}
